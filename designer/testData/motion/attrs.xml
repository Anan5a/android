  <!--Progress will call setProgress views if the method exist -->
  <attr name="progress" format="float" />

  <!--Frame Interpolation provide a per key view  time interpolation capability
  This supports linear (default) of cubic-bezier
   cubic-bezier is of the form cubic-bezier(float,float,float,float)
   where argument are x1,y1,x2,y2 representing the control on a cubic from 0,0 to 1,1
   STANDARD = "cubic(0.4, 0.0, 0.2, 1)";
   ACCELERATE = "cubic(0.4, 0.0, 1, 1)";
   DECELERATE = "cubic(0.0, 0.0, 0.2, 1)";
  -->
  <attr name="transitionEasing" format="string|enum">
    <enum name="standard" value="0"/>
    <enum name="accelerate" value="1"/>
    <enum name="decelerate" value="2"/>
    <enum name="linear" value="3"/>
  </attr>

